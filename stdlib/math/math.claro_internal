function roundDownToInt(f: float) -> int {
  var res: int;
  $$BEGIN_JAVA
    res = (int) java.lang.Math.floor(f);
  $$END_JAVA
  return res;
}
function roundUpToInt(f: float) -> int {
  var res: int;
  $$BEGIN_JAVA
    res = ((int) java.lang.Math.floor(f)) + 1;
  $$END_JAVA
  return res;
}
