
function absInt(i: int) -> int {
  var res: int;
  $$BEGIN_JAVA
    res = java.lang.Math.abs(i);
  $$END_JAVA
  return res;
}
function absFloat(f: float) -> float {
  var res: float;
  $$BEGIN_JAVA
    res = java.lang.Math.abs(f);
  $$END_JAVA
  return res;
}

